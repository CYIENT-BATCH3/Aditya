
#include<stdio.h>
struct student
{
	unsigned int x:3;
}s1;
int main()
{
	setbuf(stdout,NULL);
	s1.x=4;
	printf("size of s1=%d\n",s1.x);
	printf("size of s1=%d\n",sizeof(s1));
	s1.x=7;
	printf("size of s1=%d\n",s1.x);
	printf("size of s1=%d\n",sizeof(s1));
	s1.x=8;
	printf("size of s1=%d\n",s1.x);
	printf("size of s1=%d\n",sizeof(s1));
	return 0;
}
